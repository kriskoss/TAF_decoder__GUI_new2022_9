#:kivy 2.0.0
WindowManager:
    id: window_manager
        # https://www.youtube.com/watch?v=Prt_SKkZji8&list=WL&index=9
    FirstWindow:
        id: first
        Page1:
            id: id__Page1

    SecondWindow:
        id: second
        Page2:
            id: id__Page2
    ThirdWindow:
        id: third
        Page3:
    FourthWindow:
        id: fourth:
        Page4:
    FifthWindow:
        id: fifth:
        Page5:
            id:id__Page5
    MapWindow:
        id: mapWindow:
        Map:
            id:map
<WindowManager@ScreenManager>
<FirstWindow@Screen>:
    name: "first"
<SecondWindow@Screen>:
    name: "second"
<ThirdWindow@Screen>:
    name: "third"
<FourthWindow@Screen>:
    name: "fourth"
<FifthWindow@Screen>:
    name: "fifth"
<MapWindow@Screen>:
    name: "map"